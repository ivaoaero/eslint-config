{
  "name": "@ivao/eslint-config",
  "description": "IVAO ESLint config",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tailwind": {
      "types": "./dist/tailwind.d.ts",
      "default": "./dist/tailwind.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ivaoaero/eslint-config.git"
  },
  "author": {
    "name": "IVAO",
    "url": "https://ivao.aero"
  },
  "contributors": [
    {
      "name": "Jonas Bellmann (533870)",
      "email": "jonas.bellmann@ivao.aero",
      "url": "https://github.com/CptnJon"
    }
  ],
  "maintainers": [
    {
      "name": "Jonas Bellmann (533870)",
      "email": "jonas.bellmann@ivao.aero",
      "url": "https://github.com/CptnJon"
    }
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --cache --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint . --cache --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier . --write --cache",
    "release": "semantic-release"
  },
  "dependencies": {
    "@eslint/js": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-storybook": "0.11.4",
    "eslint-plugin-tailwindcss": "3.18.0",
    "globals": "16.0.0",
    "typescript-eslint": "8.26.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/types": "19.8.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@ivao/prettier-config": "1.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "12.0.1",
    "@types/eslint-plugin-jsx-a11y": "6.10.0",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@types/node": "22.13.10",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "semantic-release": "24.2.3",
    "tsup": "8.4.0"
  },
  "peerDependencies": {
    "eslint": "9.22.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@ivao/prettier-config",
  "packageManager": "pnpm@10.6.0+sha512.df0136e797db0cfa7ec1084e77f3bdf81bacbae9066832fbf95cba4c2140ad05e64f316cde51ce3f99ea00a91ffc702d6aedd3c0f450f895e3e7c052fe573cd8"
}
