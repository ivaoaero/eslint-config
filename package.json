{
  "name": "@ivao/eslint-config",
  "description": "IVAO ESLint config",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tailwind": {
      "types": "./dist/tailwind.d.ts",
      "default": "./dist/tailwind.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ivaoaero/eslint-config.git"
  },
  "author": {
    "name": "IVAO",
    "url": "https://ivao.aero"
  },
  "contributors": [
    {
      "name": "Jonas Bellmann (533870)",
      "email": "jonas.bellmann@ivao.aero",
      "url": "https://github.com/CptnJon"
    }
  ],
  "maintainers": [
    {
      "name": "Jonas Bellmann (533870)",
      "email": "jonas.bellmann@ivao.aero",
      "url": "https://github.com/CptnJon"
    }
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --cache --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint . --cache --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier . --write --cache",
    "release": "semantic-release"
  },
  "dependencies": {
    "@eslint/js": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-storybook": "9.1.7",
    "eslint-plugin-tailwindcss": "3.18.2",
    "globals": "16.4.0",
    "typescript-eslint": "8.44.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@ivao/prettier-config": "1.2.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@types/eslint-plugin-jsx-a11y": "6.10.0",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@types/node": "24.5.2",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "semantic-release": "24.2.9",
    "tsup": "8.5.0"
  },
  "peerDependencies": {
    "eslint": "9.36.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@ivao/prettier-config",
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef"
